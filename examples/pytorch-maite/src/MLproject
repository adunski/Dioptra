name: pytorch-maite

entry_points:
  register_model:
    parameters:
      model_provider_name: { type: string, default: "huggingface" }
      model_name: { type: string, default: "aaraki/vit-base-patch16-224-in21k-finetuned-cifar10" }
      model_task: { type: string, default: "image-classification" }
      register_model: { type: string, default: "loaded_model" }
    command: >
      PYTHONPATH=$DIOPTRA_PLUGIN_DIR validate-experiment save_model.yml && PYTHONPATH=$DIOPTRA_PLUGIN_DIR run-experiment save_model.yml
      -P model_provider_name={model_provider_name}
      -P model_name={model_name}
      -P model_task={model_task}
      -P register_model={register_model}

  infer_from_dataset:    
    parameters:
      local_dataset: { type: bool, default: false }
      dataset_kwargs: { type: str, default: "{'dataset_provider_name':'huggingface','dataset_name':'cifar10','dataset_task':'image-classification','split':'test'}"}
      local_model: { type: bool, default: false }
      model_kwargs: { type: str, default: "{'provider_name':'huggingface','model_name':'aaraki/vit-base-patch16-224-in21k-finetuned-cifar10','task':'image-classification'}"}
      metric_provider_name: { type: string, default: "torchmetrics" }
      metric_name: { type: string, default: "Accuracy" }
      metric_task: { type: string, default: "multiclass" }
      classes: { type: int, default: 10 }
      batch_size: { type: int, default: 32 }
      shape: { type: string, default: "[224,224]" }
      subset: { type: int, default: 0 }
    command: >
      PYTHONPATH=$DIOPTRA_PLUGIN_DIR validate-experiment infer_from_dataset.yml && PYTHONPATH=$DIOPTRA_PLUGIN_DIR run-experiment infer_from_dataset.yml
      -P local_dataset={local_dataset}
      -P dataset_kwargs={dataset_kwargs}
      -P local_model={local_model}
      -P model_kwargs={model_kwargs}
      -P metric_provider_name={metric_provider_name}
      -P metric_name={metric_name}
      -P metric_task={metric_task}
      -P classes={classes}
      -P batch_size={batch_size}
      -P shape={shape}
      -P subset={subset}

  infer_from_artifact:
    parameters:
      run_id: { type: string, default: "" }
      adv_data_dir: { type: string, default: "adv_testing" }
      adv_tar_name: { type: string, default: "fgm.tar.gz" }
      image_size: { type: string, default: "[224,224,3]" }
      new_size: { type: string, default: "224" }
      model_provider_name: { type: string, default: "huggingface" }
      model_name: { type: string, default: "aaraki/vit-base-patch16-224-in21k-finetuned-cifar10" }
      model_task: { type: string, default: "image-classification" }
      metric_provider_name: { type: string, default: "torchmetrics" }
      metric_name: { type: string, default: "Accuracy" }
      metric_task: { type: string, default: "multiclass" }
      classes: { type: int, default: 10 }
      batch_size: { type: int, default: 32 }
      shape: { type: string, default: "[3,224,224]" }
      subset: { type: int, default: 0 }
    command: >
      PYTHONPATH=$DIOPTRA_PLUGIN_DIR validate-experiment infer_from_artifact.yml && PYTHONPATH=$DIOPTRA_PLUGIN_DIR run-experiment infer_from_artifact.yml
      -P run_id={run_id}
      -P adv_data_dir={adv_data_dir}
      -P adv_tar_name={adv_tar_name}
      -P image_size={image_size}
      -P new_size={new_size}
      -P model_provider_name={model_provider_name}
      -P model_name={model_name}
      -P model_task={model_task}
      -P metric_provider_name={metric_provider_name}
      -P metric_name={metric_name}
      -P metric_task={metric_task}
      -P classes={classes}
      -P batch_size={batch_size}
      -P shape={shape}
      -P subset={subset}

  attack:
    parameters:
      adv_dir: { type: string, default: "adv_testing" }
      out_dir: { type: string, default: "/dioptra/data/tmp/cifar/" }
      image_size: { type: string, default: "[3,224,224]" }
      attack_name: { type: string, default: "fgm" }
      attack_library: { type: string, default: "art.attacks.evasion" }
      dataset_provider_name: { type: string, default: "huggingface" }
      dataset_name: { type: string, default: "cifar10" }
      dataset_task: { type: string, default: "image-classification" }
      split: { type: string, default: "test" }
      model_provider_name: { type: string, default: "huggingface" }
      model_name: { type: string, default: "aaraki/vit-base-patch16-224-in21k-finetuned-cifar10" }
      model_task: { type: string, default: "image-classification" }
      metric_provider_name: { type: string, default: "torchmetrics" }
      metric_name: { type: string, default: "Accuracy" }
      metric_task: { type: string, default: "multiclass" }
      classes: { type: int, default: 10 }
      batch_size: { type: int, default: 32 }
      shape: { type: string, default: "[224,224]" }
      subset: { type: int, default: 0 }
      save_original: { type: bool, default: true }
      attack_kwargs: { type: string, default: "{eps:0.3}" }
    command: >
      PYTHONPATH=$DIOPTRA_PLUGIN_DIR validate-experiment attack.yml && PYTHONPATH=$DIOPTRA_PLUGIN_DIR run-experiment attack.yml
      -P adv_dir={adv_dir}
      -P out_dir={out_dir}
      -P split={split}
      -P image_size={image_size}
      -P attack_library={attack_library}
      -P attack_name={attack_name}
      -P dataset_provider_name={dataset_provider_name}
      -P dataset_name={dataset_name}
      -P dataset_task={dataset_task}
      -P model_provider_name={model_provider_name}
      -P model_name={model_name}
      -P model_task={model_task}
      -P metric_provider_name={metric_provider_name}
      -P metric_name={metric_name}
      -P metric_task={metric_task}
      -P classes={classes}
      -P batch_size={batch_size}
      -P shape={shape}
      -P subset={subset}
      -P save_original={save_original}
      -P attack_kwargs={attack_kwargs}
